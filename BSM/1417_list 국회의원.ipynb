    "n =int(input())\n",
    "target = int(input())\n",
    "num_list =  [int(input()) for _ in range(n-1)]\n",
    "count = 0\n",
    "\n",
    "if n == 1 :\n",
    "    pass\n",
    "else :\n",
    "    while target <= max(num_list) :\n",
    "        target +=1\n",
    "        count +=1\n",
    "        num_list[num_list.index(max(num_list))] -= 1\n",
    "\n",
    "print(count)"
