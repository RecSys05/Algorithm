   "source": [
    "def fac(x) :\n",
    "    mul = 1\n",
    "    for i in range(1,x+1) :\n",
    "        mul *= i\n",
    "\n",
    "    return mul\n",
    "\n",
    "m,n  = map(int,input().split())\n",
    "\n",
    "print(int(fac(m) / fac(n) / fac(m-n)))"
   ]
  }
